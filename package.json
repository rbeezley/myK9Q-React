{
  "name": "myk9q-react",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:offline-first": "vitest run --coverage useOptimisticScoring EntryList offline-first-pattern-consistency",
    "setup": "node scripts/setup-dev.cjs",
    "install-hooks": "node scripts/install-git-hooks.cjs",
    "postinstall": "node scripts/install-git-hooks.cjs",
    "audit:design": "node .claude/skills/myk9q-design-system/tools/audit-design-system.js",
    "parse:rules": "tsx scripts/parse-akc-rulebook.ts",
    "seed:rules": "tsx scripts/seed-akc-rules.ts",
    "validate:rules": "tsx scripts/validate-rules-accuracy.ts",
    "test:e2e": "npx playwright test --config=playwright.e2e.config.ts",
    "test:e2e:ui": "npx playwright test --config=playwright.e2e.config.ts --ui",
    "test:e2e:headed": "npx playwright test --config=playwright.e2e.config.ts --headed",
    "test:e2e:debug": "npx playwright test --config=playwright.e2e.config.ts --debug",
    "playwright:install": "npx playwright install chromium"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@supabase/supabase-js": "^2.81.1",
    "@tanstack/query-sync-storage-persister": "^5.90.11",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-query-persist-client": "^5.90.11",
    "@tanstack/react-virtual": "^3.13.12",
    "ai-labs-claude-skills": "^2.0.1",
    "canvas-confetti": "^1.9.4",
    "clsx": "^2.1.1",
    "comlink": "^4.4.2",
    "date-fns": "^4.1.0",
    "idb": "^8.0.3",
    "lucide-react": "^0.555.0",
    "pdfjs-dist": "^5.4.394",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "recharts": "^3.4.1",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.9",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fake-indexeddb": "^6.2.5",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "puppeteer": "^24.30.0",
    "react-router-dom": "^7.9.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.9",
    "workbox-window": "^7.3.0"
  }
}
